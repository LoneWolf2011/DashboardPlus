## PHP ini overrides
php_value auto_prepend_file "C:/xampp/htdocs/antdb/Src/Config/common.php"
php_value max_execution_time 600
php_value post_max_size 1024M 
php_value upload_max_filesize 1024M 
php_value max_execution_time 3600
php_value max_input_time 3600 
php_value memory_limit 1024M 

## Error page redirect
ErrorDocument 403 /antdb/View/Errors/page_403.php
ErrorDocument 404 /antdb/View/Errors/page_404.php
ErrorDocument 500 /antdb/View/Errors/page_500.php

## Deny access tot .htaccess
<Files .htaccess>
	order allow,deny
	deny from all
</Files>

## Disable directory listing. If index.php is not found use /antdb/View/index.php
DirectoryIndex index.php /antdb/View/

## Deny access to extension .map files
<Files ~ "\.map$">
	Order allow,deny
	Deny from all
	Satisfy all
</Files>

## Deny access to .ini files
<Files ~ "\.ini$">
    Order allow,deny
    Deny from all
 </Files>

## Redirect access /src/ to 403 page
RedirectMatch 403 ^/src/.*$

